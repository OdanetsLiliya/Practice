{
    "env": {
        "browser": true
    },
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-plusplus": "off",
        "func-names": "off",
        "no-multi-assign": "off",
        "no-console": "off",
        "class-methods-use-this": "off",
        "no-underscore-dangle": "off",
        "no-multi-str": "off",
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "no-param-reassign": "off",
        "eqeqeq": "warn",
        "no-loop-func": "warn",
        "prefer-template": "off",
        "no-shadow": "warn",
        "prefer-promise-reject-errors": "warn",
        "no-mixed-operators": "warn",
        "consistent-return": "off"
    }
}